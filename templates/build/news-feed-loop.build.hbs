\{{#each this}}
<div class="news-feed-list-item mix \{{data.[classes]}}" data-entry-id="\{{id}}">
  <div class="news-feed-item-content">
    <section class="slide-under">
      <div class="banner" data-height="20vh" data-height-expanded="45vh" style="height: 20vh; background-image: url('\{{data.[Image]}}');"></div>
      <i class="fa fa-picture-o"></i>
    </section>
    <div class="container-fluid slide-over">
      <div class="news-feed-item-inner-content" data-position="20vh" data-position-expanded="40vh" style="top: 20vh">
        <div class="news-feed-info-holder">
          <div class="news-feed-date-category">\{{data.[Date]}} \{{#ifCond data.[Date] '&&' data.[Categories]}}/\{{/ifCond}} \{{data.[Categories]}}</div>
          \{{#if likesEnabled}}
          <div class="news-feed-like-holder news-feed-like-holder-\{{id}}">
            <!-- Like button will be here -->
            <!-- Like placeholder -->
            <i class="fa fa-heart-o fa-lg like-placeholder"></i>
          </div>
          \{{/if}}
        </div>
        <h2 class="news-feed-item-title">\{{data.[Title]}}</h2>
        <div class="news-feed-item-description">
          \{{{data.[Content]}}}
        </div>
      </div>
    </div>
    <div class="news-feed-item-close-btn-wrapper">
      <div class="news-feed-item-close-btn"><i class="fa fa-times"></i></div>
    </div>
  </div>
</div>
\{{/each}}