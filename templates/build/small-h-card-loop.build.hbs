\{{#each this}}
  <div class="small-h-card-list-item" data-entry-id="\{{id}}" tabindex="0">
    <div class="small-h-card-list-image">
      <div class="small-h-card-list-detail-wrapper" data-entry-id="\{{id}}">
        <div class="small-h-card-list-detail-image-wrapper">
          <div class="small-h-card-list-detail-image" style="background-image: url('\{{auth [Image]}}')"></div>
          \{{#unless [Image]}}<i class="fa fa-user"></i>\{{/unless}}
        </div>
      </div>
    </div>
    <div class="small-h-card-list-item-text">\{{[First Name]}} \{{[Last Name]}}</div>
  </div>
\{{/each}}