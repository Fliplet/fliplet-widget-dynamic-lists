\{{#each this}}
  <div class="small-h-card-list-item">
    <div class="small-h-card-list-image">
      <div class="small-h-card-list-detail-wrapper" data-entry-id="\{{id}}">
        <div class="small-h-card-list-detail-image-wrapper">
          <div class="small-h-card-list-detail-image">
            <img src="\{{data.[Image]}}">
          </div>
        </div>

        <!-- Don't delete this -->
        <!-- This is where the detail view will be added -->
        \{{{ data.[profileHTML] }}}

      </div>
    </div>
    <div class="small-h-card-list-item-text">\{{data.[First Name]}} \{{data.[Last Name]}}</div>
  </div>
\{{/each}}