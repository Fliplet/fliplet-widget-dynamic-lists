{
  "extends": "airbnb",
  "env": {
    "node": true,
    "es6": true,
    "jasmine": true,
    "browser": true,
    "jquery": true
  },
  "globals": {
    "_": true,
    "Fliplet": true,
    "interfaceBrowser": true,
    "buildBrowser": true,
    "interfaceUrl": true,
    "buildUrl": true,
    "widgetInstance": true,
    "buildSelector": true,
    "Handlebars": true
  },
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "block-scoped-var": 2,
    "brace-style": [2, "1tbs"],
    "camelcase": 1,
    "computed-property-spacing": [2, "never"],
    "curly": 2,
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "func-names": [1, "as-needed"],
    "max-depth": [1, 3],
    "max-len": [1, 80],
    "max-statements": [1, 15],
    "new-cap": 1,
    "no-extend-native": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "no-unused-vars": 1,
    "no-underscore-dangle": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-var": 0,
    "prefer-template": 1,
    "quotes": [2, "single", "avoid-escape"],
    "quote-props": 1,
    "semi": [2, "always"],
    "keyword-spacing": [2, {"before": true, "after": true}],
    "space-unary-ops": 2,
    "comma-dangle": [2, "never"],
    "consistent-return": [0],
    "prefer-arrow-callback": [0],
    "no-param-reassign": [1]
  }
}
