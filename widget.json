      "vendor/select2.css",
      "vendor/select2.full.js",
      "js/interface.templates.js",
      "js/build.templates.js",
      "js/layout-mapping/list-layouts.js",
      "js/layout-mapping/layout-mapping.js",
      "js/default-configs/layouts-config.js",
      "js/default-table-data/layouts-table-config.js",
      "js/interface.js",
      "js/interface-lists.js",
      "css/interface.css"
    ]
