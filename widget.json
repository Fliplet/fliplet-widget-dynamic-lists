    "commentsDataSourceId:dataSource",
    "userDataSourceId:dataSource",
    "addEntryLinkAction.page:page",
    "editEntryLinkAction.page:page",
    "summaryLinkAction.page:page",
    "summary-fields.$.folder.selectFiles.$.id:mediaFolder",
    "detailViewOptions.$.folder.selectFiles.$.id:mediaFolder",
    "summary-fields.$.appFolderId:app",
    "detailViewOptions.$.appFolderId:app",
    "summary-fields.$.organizationFolderId:organization",
    "detailViewOptions.$.organizationFolderId:organization",
    "dataSourceViews.$.id:dataSourceView"
