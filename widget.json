  "build": {
    "dependencies": [
      "fliplet-core",
      "fliplet-session",
      "fliplet-datasources",
      "fliplet-media",
      "font-awesome",
      "bootstrap",
      "moment",
      "lodash",
      "handlebars",
      "animate-css",
