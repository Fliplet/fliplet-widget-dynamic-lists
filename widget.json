      "vendor/ellipsed.js",
      "js/default-configs/layouts-config.js",
      "js/build-lists.js",
      "js/build.js",
      "css/build.css",

      "css/layout-css/small-card-style.upload.css",
      "css/layout-css/news-feed-style.upload.css",
      "css/layout-css/agenda-style.upload.css",
